{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/pkg/(.*)",
      "dest": "/public/js/pkg/$1",
      "headers": {
        "Content-Type": "application/wasm"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)\\.mp3",
      "dest": "/public/$1.mp3"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}