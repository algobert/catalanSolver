{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/gml-files",
      "dest": "server.js"
    },
    {
      "src": "/game",
      "dest": "public/game.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}